{
  "env": {
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": [],
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error"
  },
  "ignorePatterns": [
    "build/**/*.js"
  ]
}